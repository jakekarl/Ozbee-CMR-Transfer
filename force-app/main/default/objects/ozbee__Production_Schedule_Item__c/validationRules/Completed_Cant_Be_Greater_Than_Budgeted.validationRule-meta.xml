<?xml version="1.0" encoding="UTF-8" ?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Completed_Cant_Be_Greater_Than_Budgeted</fullName>
    <active>true</active>
    <errorConditionFormula
  >Completed__c &gt;  Quantity__c</errorConditionFormula>
    <errorDisplayField>Completed__c</errorDisplayField>
    <errorMessage
  >Completed cannot be greater than budgeted quantity</errorMessage>
</ValidationRule>
